{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "module": "node16",
    "moduleResolution": "node16",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["src"]
}
