{
  "name": "rollup-plugin-github-code-import",
  "version": "0.1.0",
  "description": "Rollup plugin that imports code from a GitHub URL",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --check --write .",
    "format:fail": "prettier --check .",
    "lint": "eslint --cache .",
    "test": "vitest",
    "build": "tsc --project tsconfig.build.json"
  },
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "markdown",
    "code-block"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "author": {
    "name": "Hao Cheng",
    "email": "hcheng636@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haocheng6/rollup-plugin-github-code-import.git"
  },
  "packageManager": "pnpm@8.12.1",
  "dependencies": {
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-github-code-import": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "3.1.1",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
